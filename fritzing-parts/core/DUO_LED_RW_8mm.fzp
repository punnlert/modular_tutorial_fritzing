<?xml version="1.0" encoding="UTF-8"?>
<module fritzingVersion="1.0.4" moduleId="DUO_LED_RW_8mm">
    <version>4</version>
    <author>Kjell Morgenstern</author>
    <title>DUO LED RW</title>
    <label>LED</label>
    <date>2025-03-26</date>
    <tags>
        <tag>DUO LED</tag>
        <tag>DUO color</tag>
        <tag>indicator</tag>
    </tags>
    <properties>
        <property name="package">8 mm [THT]</property>
        <property name="family">LED</property>
        <property name="pin order">rw</property>
        <property name="polarity">common cathode</property>
    </properties>
    <description>A 8mm DUO-color LED with red and white inside.

Pin order: red - com. cathode - white</description>
    <views>
        <breadboardView>
            <layers image="breadboard/DUO_LED_RW_8mm_flat_breadboard.svg">
                <layer layerId="breadboard"/>
            </layers>
        </breadboardView>
        <schematicView>
            <layers image="schematic/DUO_LED_RW_schematic.svg">
                <layer layerId="schematic"/>
            </layers>
        </schematicView>
        <pcbView>
            <layers image="pcb/DUO_LED_8mm_pcb.svg">
                <layer layerId="copper0"/>
                <layer layerId="copper1"/>
                <layer layerId="silkscreen"/>
            </layers>
        </pcbView>
        <iconView>
            <layers image="icon/DUO_LED_RW_8mm_flat_icon.svg">
                <layer layerId="icon"/>
            </layers>
        </iconView>
    </views>
    <connectors>
        <connector id="connector4" type="male" name="white anode">
            <description>anode pin for white LED</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector4pin" legId="connector4leg"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector4pin"/>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector4pad"/>
		<p layer="copper1" svgId="connector4pad"/>
                </pcbView>
            </views>
        </connector>
        <connector id="connector0" type="male" name="red anode">
            <description>anode pin for red LED</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector0pin" legId="connector0leg"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector0pin"/>
                </schematicView>
                <pcbView>
                    <p layer="copper1" svgId="connector0pad"/>
                    <p layer="copper0" svgId="connector0pad"/>
                </pcbView>
            </views>
        </connector>
        <connector id="connector1" type="male" name="common cathode">
            <description>cathode pin for every color</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector1pin" legId="connector1leg"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector1pin"/>
                </schematicView>
                <pcbView>
                    <p layer="copper1" svgId="connector1pad"/>
                    <p layer="copper0" svgId="connector1pad"/>
                </pcbView>
            </views>
        </connector>
    </connectors>
</module>
